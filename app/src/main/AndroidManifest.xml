<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.laink.filmFrame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_question"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_question_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3531309073175655~9641708618" />

        <activity
            android:name="com.laink.filmFrame.StartActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:theme="@style/AppThemeStart">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.laink.filmFrame.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.laink.filmFrame.MainActivity"
            android:theme="@style/AppSettingsTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.laink.filmFrame.MainActivity" />
        </activity>
        <activity
            android:name="com.laink.filmFrame.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>